{
  "nodes": {
    "app" : {
      "ami": "ami-96666ff5",
      "security_groups": ["ssh2"],
      "type": "t2.micro",
      "tags": { "name": "app" },
      "attach_volume": false,
      "volume": "vol-02b6e03aa9a40d5f3",
      "volume_mount_point": "/dev/xvdb",
      "volume_mount_dir": "/data",
      "ansible_host_groups": [
        "docker",
        "global_env",
        "mount_vol"
      ],
      "env_injection": [
        "INST_NAME=app"
      ]
    }
  }
}
